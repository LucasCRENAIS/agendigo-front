img.chat-pic{
  -webkit-animation-duration:3s;
    animation-duration:3s;
    -webkit-animation-name:scrollChat;
    animation-name:scrollChat;
    -webkit-animation-timing-function: cubic-bezier(1.000, 0.005, 0.305, 1.000);
    animation-timing-function: cubic-bezier(1.000, 0.005, 0.305, 1.000); 
    -webkit-animation-direction: alternate; 
            animation-direction: alternate;
    -webkit-animation-iteration-count: infinite;
            animation-iteration-count: infinite;
    -webkit-animation-fill-mode: none;
            animation-fill-mode: none;
       

}
@keyframes scrollChat {

  10% {
      -webkit-transform: translateY(0%);
          -ms-transform: translateY(0%);
              transform: translateY(0%);
  }

  100% {
      -webkit-transform: translateY(calc(-30%));
          -ms-transform: translateY(calc(-30%));
              transform: translateY(calc(-30%));
  }



}



.mouse {

  border: 1.3px solid #C6C6C6;
  border-radius: 60px;
  margin: 50px auto 10px auto;
  position: relative;
  z-index: 3;
  background-color: rgba(255,255,255,0.8);

 
}

.mouse {
  background: rgba(255,255,255) -webkit-gradient(linear, left top, left bottom, from(transparent), color-stop(50%, transparent), color-stop(50%, #ffffff), to(#ffffff));
  background: rgba(255,255,255) linear-gradient(transparent 0%, transparent 50%, #6C63FF 50%, #6C63FF 100%);
  position: relative;
  height: 40px;
  width: 28px;
  border-radius: 100px;
  background-size: 100% 200%;
  -webkit-animation: colorSlide 7s linear infinite, nudgeMouse 7s ease-out infinite;
          animation: colorSlide 7s linear infinite, nudgeMouse 7s ease-out infinite;
}
.mouse:before, .mouse:after {
  content: "";
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
}
.mouse:before {
  height: 36px;
  width: 24px;
  background-color:  rgba(255,255,255);
  border-radius: 100px;
}
.mouse:after {
  background-color: #6C63FF;
  width: 4px;
  height: 4px;
  top: 15px;
  border-radius: 100%;
  -webkit-animation: trackBallSlide 7s linear infinite;
          animation: trackBallSlide 7s linear infinite;
}

@-webkit-keyframes colorSlide {
  0% {
    background-position: 0% 100%;
  }
  20% {
    background-position: 0% 0%;
  }
  21% {
    background-color: rgba(255,255,255);
  }
  29.99% {
    background-color: #6C63FF;
    background-position: 0% 0%;
  }
  30% {
    background-color: rgba(255,255,255);
    background-position: 0% 100%;
  }
  50% {
    background-position: 0% 0%;
  }
  51% {
    background-color: rgba(255,255,255);
  }
  59% {
    background-color: #6C63FF;
    background-position: 0% 0%;
  }
  60% {
    background-color: rgba(255,255,255);
    background-position: 0% 100%;
  }
  80% {
    background-position: 0% 0%;
  }
  81% {
    background-color: rgba(255,255,255);
  }
  90%, 100% {
    background-color: #6C63FF;
  }
}

@keyframes colorSlide {
  0% {
    background-position: 0% 100%;
  }
  20% {
    background-position: 0% 0%;
  }
  21% {
