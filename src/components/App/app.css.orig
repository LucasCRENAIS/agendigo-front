.animate__bounceIn{
  animation: bounceIn; /* referring directly to the animation's @keyframe declaration */
  animation-duration: 2s;


}

.animate__bounceIn:nth-child(2n){
  animation: bounceIn; /* referring directly to the animation's @keyframe declaration */
  animation-duration: 1s;


}

